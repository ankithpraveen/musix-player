<!DOCTYPE html>
<html>
<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="./mdb/js/mdb.min.js"></script>
<link href="./mdb/css/mdb.min.css" rel="stylesheet" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" />
<script src="dash.js"></script>
<head><title id="title">My Library</title></head>
<body onload="showpl()" id="bg">
  <!-- Navbar -->
  <!-- Container wrapper -->
  <div class="bg-image" style="
        background-image: linear-gradient(rgb(0, 0, 0), rgb(37, 37, 37));
        height: 100vh;
      " id="main">
    <!-- Navbar -->
    <!-- Container wrapper -->
    <nav class="navbar navbar-expand-lg navbar-dark bg-black sticky-top">
      <div class="container-fluid">
        <!-- Toggle button -->
        <button class="navbar-toggler" type="button" data-mdb-toggle="collapse"
          data-mdb-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
          aria-label="Toggle navigation">
          <i class="fa fa-bars"></i>
        </button>

        <!-- Collapsible wrapper -->
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <!-- Navbar brand -->
          <a class="navbar-brand mt-2 mt-lg-0" href="#">
            <img src="https://i.pinimg.com/474x/1e/9d/c2/1e9dc24db36c9226da76585bd0314b19.jpg" height="40" alt=""
              loading="lazy"/>
          </a>
          <!-- Left links -->
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
            <li class="nav-item">
              <a class="nav-link" style="color: white" href="javascript:void(0);" onclick="dash()">Dashboard</a>
            </li>
            <li class="nav-item">
              <a class="nav-link active" style="color: rgb(54, 176, 233)" href="/library">
                My Library</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" style="color: white" href="/newSong">Upload New Song</a>
            </li>
          </ul>
          <!-- Left links -->
        </div>
        <!-- Collapsible wrapper -->

        <!-- Right elements -->
        <!-- Avatar -->
        <a style="padding-right: 0.75%" class="dropdown-toggle d-flex align-items-center hidden-arrow" href="#"
          id="navbarDropdownMenuLink" role="button" data-mdb-toggle="dropdown" aria-expanded="false">
          <i class="fa fa-user fa-lg"></i>
          <!-- <img
              src="https://mdbootstrap.com/img/new/avatars/2.jpg"
              class="rounded-circle"
              height="40"
              alt=""
              loading="lazy"
            /> -->
        </a>
        <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdownMenuLink">
          <li>
            <span class="dropdown-item-text" href="#"><%=name%></span>
          </li>
          <li>
            <span class="dropdown-item-text" href="#"><%=email%></span>
          </li>
          <li>
            <a class="dropdown-item" href="/logout">Logout</a>
          </li>
        </ul>
      </div>
      <!-- Right elements -->
    </nav>
  
  <!-- Container wrapper -->

  <!-- Navbar -->
  <div id="load"></div>
  <div id="body">
    <div class="container" id="container">
      <h4 style="color: white">My Playlists</h4>
      <div id="carouselExampleControls1" class="carousel slide" data-mdb-ride="carousel">
        <div class="carousel-inner" id="car">
          <div class="carousel-item active">
            <div class="row" id="pls0">
              <div class="col-sm-3" style="padding-top: 1%"></div>
              <div class="col-sm-2" style="padding-top: 1%">
                <div class="card" style="background-color:transparent;">
                  <div class="bg-image hover-overlay ripple" data-mdb-ripple-color="light">
                    <img
                      src="https://i.picsum.photos/id/693/200/150.jpg?grayscale&hmac=QDXoEU04DyaG7M8c842-qtEs0m1MCM9_XyYNS8BLcB8"
                      class="card-img-top rounded-bottom" alt="..." />
                    <a href="/newpldets">
                      <div class="
                              mask
                              d-flex
                              justify-content-center
                              align-items-center
                            " style="background-color: rgba(0, 0, 0, 0.45)">
                        <button class="btn btn-primary btn-floating bg-white">
                          <i class="fa fa-plus-circle fa-3x" style="color: #0f64f2"></i>
                        </button>
                      </div>
                    </a>
                  </div>

                  <h5 class="card-title text-white" style="padding-top:10px;padding-left:5px;">New Playlist</h5>

                </div>
              </div>
            </div>
          </div>
        </div>
        <button class="carousel-control-prev" type="button" data-mdb-target="#carouselExampleControls1"
          data-mdb-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-mdb-target="#carouselExampleControls1"
          data-mdb-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="visually-hidden">Next</span>
        </button>
      </div>
      <br />
      <br />
      <div id="carousel2"></div>
    </div>
  </div>
  </div>
  <p id="email" hidden>
    <%=email%>
</p>
  <p id="name" hidden>
    <%=name%>
</p>
</body>

<footer class="text-center text-white fixed-bottom" id="footer" style="
      background-image: linear-gradient(to top, rgb(0, 0, 0), rgb(31, 31, 31));
      display: none;
    ">
  <!-- Grid container -->

  <div class="container p-2">
    <div class="range">
      <input style="width: 100%; height: 1px" type="range" id="seek1" min="0" max="1" value="0" step="0.01" />
    </div>
    <div class="row">
      <div class="col-md-4">
        <div class="d-flex justify-content-center">
          <div class="p-2">
            <p id="duration">0:00</p>
          </div>
          <div class="p-2">
            <p id="nplaying"></p>
          </div>
        </div>
      </div>
      <div class="col-md-4">
        <div class="d-flex justify-content-center">
          <div class="p-2">
            <button onclick="prev()" class="btn btn-floating">
              <i class="fas fa-step-backward fa-2x" style="color: #ffffff"></i>
            </button>
          </div>
          <div class="p-2" id="playpause">
            <button onclick="pause()" class="btn btn-primary btn-floating bg-white">
              <i class="fa fa-pause-circle fa-3x" style="color: #0f64f2"></i>
            </button>
          </div>
          <div class="p-2">
            <button onclick="next()" class="btn btn-floating">
              <i class="fas fa-step-forward fa-2x" style="color: #ffffff"></i>
            </button>
          </div>
        </div>
      </div>
      <div class="col-md-4">
        <div class="d-flex justify-content-center">
          <div class="p-2">
            <i class="fa fa-volume-up" style="padding-top: 8px"></i>
          </div>
          <div class="p-2">
            <input style="
                  position: relative;
                  right: auto;
                  padding: auto;
                  height: 3px;
                " type="range" id="volume1" min="0" max="2" value="1" step="0.01" />
          </div>
        </div>
      </div>
    </div>
  </div>
</footer>

</html>