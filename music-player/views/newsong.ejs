<!DOCTYPE html>
<html>

<body>
  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  <script src="./mdb/js/mdb.min.js"></script>
  <link href="./mdb/css/mdb.min.css" rel="stylesheet" />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />
  <script src="newsong.js"></script>

  <!-- Navbar -->
  <!-- Container wrapper -->

  <nav class="navbar navbar-expand-lg navbar-dark bg-black sticky-top">
    <div class="container-fluid">
      <!-- Toggle button -->
      <button class="navbar-toggler" type="button" data-mdb-toggle="collapse" data-mdb-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <i class="fa fa-bars"></i>
      </button>

      <!-- Collapsible wrapper -->
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <!-- Navbar brand -->
        <a class="navbar-brand mt-2 mt-lg-0" href="#">
          <img src="https://i.pinimg.com/474x/1e/9d/c2/1e9dc24db36c9226da76585bd0314b19.jpg" height="40" alt=""
            loading="lazy" />
        </a>
        <!-- Left links -->
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item">
            <a class="nav-link active" style="color: rgb(54, 176, 233)" href="/dashboard">Dashboard</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" style="color: white" href="/library"> My Library</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" style="color: white" href="/newSong">Upload New Song</a>
          </li>
        </ul>
        <!-- Left links -->
      </div>
      <!-- Collapsible wrapper -->

      <!-- Right elements -->
      <div class="d-flex align-items-center">
        <form class="d-flex" style="padding-right: 5%">
          <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search" type="text"
            class="form-control" onkeyup="dynamic_search(event)" onclick="getSongs()" id="sname" value="" />
        </form>
        <!-- Avatar -->
        <a style="padding-right: 5%" class="dropdown-toggle d-flex align-items-center hidden-arrow" href="#"
          id="navbarDropdownMenuLink" role="button" data-mdb-toggle="dropdown" aria-expanded="false">
          <i class="fa fa-user fa-lg"></i>
          <!-- <img
              src="https://mdbootstrap.com/img/new/avatars/2.jpg"
              class="rounded-circle"
              height="40"
              alt=""
              loading="lazy"
            /> -->
        </a>
        <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdownMenuLink">
          <li>
            <a class="dropdown-item" href="#">My profile</a>
          </li>
          <li>
            <a class="dropdown-item" href="#">Settings</a>
          </li>
          <li>
            <a class="dropdown-item" href="/logout">Logout</a>
          </li>
        </ul>
      </div>
      <!-- Right elements -->
    </div>
    <!-- Container wrapper -->
  </nav>

  <!-- Navbar -->

  <div style="
        background-image: linear-gradient(rgb(0, 0, 0), rgb(37, 37, 37));
        height: 200vh;
      ">
    <br />

    <div id="body">
      <p id="email" hidden>
        <%=email%>
      </p>


      <div class="container">
        <h4 style="color:white">Upload New Song</h4>
        <div class="row" id="pls">
          
          <form>
          <div class="form-outline mb-4">
            <input type="text" id="sname" class="form-control"  style="border: 1px solid white;"/>
            <label class="form-label text-white" for="sname"  style="margin-left: 20px;">Song Name</label>
          </div>
          <div class="form-outline mb-4">
            <input type="file" class="form-control" id="file1"  style="border: 1px solid white;"/>
          </div>
            <button type="submit" class="btn btn-primary btn-block" onclick="upload()">Upload</button>
        </form>
        </div>
      </div>
    </div><br><br>

</body>

<footer class="text-center text-white fixed-bottom" style="
      background-image: linear-gradient(to top, rgb(0, 0, 0), rgb(31, 31, 31));
    ">
  <!-- Grid container -->

  <div class="container p-2">

    <div class="range">
      <input style="width: 100%;height:1px" type="range" id="seek1" min="0" max="1" value="0" step="0.0001" />
    </div>
    <div class="d-flex justify-content-around">
      <div class="p-1">
        <p id="duration">0:00</p>
      </div>
      <div class="p-1" id="playpause">
        <button onclick="pause()" class="btn btn-primary btn-floating bg-white">
          <i class="fa fa-pause-circle fa-3x" style="color:#0f64f2"></i>
        </button>
      </div>
      <div class="p-1">
        <i class="fa fa-volume-up"></i>
        <input style="position: relative; right: auto;padding:auto" type="range" id="volume1" min="0" max="2" value="1"
          step="0.01" />
      </div>
    </div>
  </div>
</footer>

</html>









