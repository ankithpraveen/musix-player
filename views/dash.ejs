<!DOCTYPE html>
<html>
  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  <script src="./mdb/js/mdb.min.js"></script>
  <link href="./mdb/css/mdb.min.css" rel="stylesheet" />
  <link
    rel="stylesheet"
    href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"
  />
  <script src="dash.js"></script>
  <head><title id="title">Algo-Rhythm</title></head>
  <body id="bg" onload="getSongs()">
    <!-- Navbar -->
    <!-- Container wrapper -->
    <div
      class="bg-image"
      style="
        background-image: linear-gradient(rgb(0, 0, 0), rgb(37, 37, 37));
        height: 100vh;
      "
      id="main"
    >
      <nav class="navbar navbar-expand-lg navbar-dark bg-black sticky-top">
        <div class="container-fluid">
          <!-- Toggle button -->
          <button
            class="navbar-toggler"
            type="button"
            data-mdb-toggle="collapse"
            data-mdb-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent"
            aria-expanded="false"
            aria-label="Toggle navigation"
          >
            <i class="fa fa-bars"></i>
          </button>

          <!-- Collapsible wrapper -->
          <div class="collapse navbar-collapse" id="navbarSupportedContent">

            <!-- Left links -->
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
              <li class="nav-item">
                <a
                  class="nav-link active"
                  style="color: rgb(54, 176, 233)"
                  href="/dashboard"
                  >Dashboard</a
                >
              </li>
              <li class="nav-item">
                <a
                  class="nav-link"
                  style="color: white"
                  href="javascript:void(0);"
                  onclick="lplaylists()"
                >
                  My Library</a
                >
              </li>
              <li class="nav-item">
                <a class="nav-link" style="color: white" href="/newSong"
                  >Upload New Song</a
                >
              </li>
            </ul>
            <!-- Left links -->
          </div>
          <!-- Collapsible wrapper -->

          <!-- Right elements -->
          <div class="d-flex align-items-center">
            <form class="d-flex" style="padding-right: 5%">
              <input
                class="form-control me-2"
                type="search"
                placeholder="Search"
                aria-label="Search"
                type="text"
                class="form-control"
                onkeyup="dynamic_search(event)"
                id="sname"
                value=""
              />
            </form>
            <!-- Avatar -->
            <a
              style="padding-right: 5%"
              class="dropdown-toggle d-flex align-items-center hidden-arrow"
              href="#"
              id="navbarDropdownMenuLink"
              role="button"
              data-mdb-toggle="dropdown"
              aria-expanded="false"
            >
              <i class="fa fa-user fa-lg"></i>
              <!-- <img
              src="https://mdbootstrap.com/img/new/avatars/2.jpg"
              class="rounded-circle"
              height="40"
              alt=""
              loading="lazy"
            /> -->
            </a>
            <ul
              class="dropdown-menu dropdown-menu-end"
              aria-labelledby="navbarDropdownMenuLink"
            >
              <li>
                <span class="dropdown-item-text" href="#"><%=name%></span>
              </li>
              <li>
                <span class="dropdown-item-text" href="#"><%=email%></span>
              </li>
              <li>
                <a class="dropdown-item" href="/logout">Logout</a>
              </li>
            </ul>
          </div>
          <!-- Right elements -->
        </div>
        <!-- Container wrapper -->
      </nav>

      <!-- Navbar -->

      <div class="container">
        <div class="row" id="sugg">
          
        </div>
      </div>

      <br /><br />
    </div>
    <p id="email" hidden>
      <%=email%>
  </p>
    <p id="name" hidden>
      <%=name%>
  </p>
  </body>

  <footer
    class="text-center text-white fixed-bottom"
    id="footer"
    style="
      background-image: linear-gradient(to top, rgb(0, 0, 0), rgb(31, 31, 31));
      display: none;
    "
  >
    <!-- Grid container -->

    <div class="container p-2">
      <div class="range">
        <input
          style="width: 100%; height: 1px"
          type="range"
          id="seek1"
          min="0"
          max="1"
          value="0"
          step="0.001"
        />
      </div>
      <div class="row">
        <div class="col-md-4"><div class="d-flex justify-content-center">
            <div class="p-2"><p id="duration">0:00</p></div><div class="p-2"><p id="nplaying"></p></div></div>
        </div>
        <div class="col-md-4">
          <div class="d-flex justify-content-center">
            <div class="p-2">
              <button
                onclick="prev()"
                class="btn btn-floating"
              >
                <i class="fas fa-step-backward fa-2x" style="color: #ffffff"></i>
              </button>
            </div>
            <div class="p-2" id="playpause">
              <button
                onclick="pause()"
                class="btn btn-primary btn-floating bg-white"
              >
                <i class="fa fa-pause-circle fa-3x" style="color: #0f64f2"></i>
              </button>
            </div>
            <div class="p-2">
              <button
                onclick="next()"
                class="btn btn-floating"
              >
                <i class="fas fa-step-forward fa-2x" style="color: #ffffff"></i>
              </button>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="d-flex justify-content-center">
            <div class="p-2">
              <i class="fa fa-volume-up" style="padding-top: 8px"></i>
            </div>
            <div class="p-2">
              <input
                style="
                  position: relative;
                  right: auto;
                  padding: auto;
                  height: 3px;
                "
                type="range"
                id="volume1"
                min="0"
                max="2"
                value="1"
                step="0.01"
              />
            </div>
          </div>
        </div>
      </div>
    </div>
  </footer>
</html>
